<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Console.TableResult.NextPage" />
  <action id="Console.TableResult.PreviousPage" />
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift alt w" />
  </action>
  <action id="ExportSettings">
    <keyboard-shortcut first-keystroke="alt open_bracket" second-keystroke="alt close_bracket" />
  </action>
  <action id="Move">
    <keyboard-shortcut first-keystroke="f6" />
    <keyboard-shortcut first-keystroke="alt m" />
  </action>
  <action id="NewFile">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="NextOccurence" />
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="alt period" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="alt comma" />
  </action>
  <action id="PreviousOccurence" />
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt comma" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="Vcs.ShowHistoryForBlock">
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="WelcomScreen.ManageLicense">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="tasks.close" />
</keymap>